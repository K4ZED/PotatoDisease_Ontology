:hasSymptom a owl:ObjectProperty ;
  rdfs:range :Symptom ;
  rdfs:label "has symptom"@en .

:hasDisease a owl:ObjectProperty ;
  rdfs:range :PotatoDisease ;
  rdfs:label "has disease"@en .

:indicatesDisease a owl:ObjectProperty ;
