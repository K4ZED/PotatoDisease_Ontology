@prefix : <http://example.org/potato#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://example.org/potato#> .

<http://example.org/potato> rdf:type owl:Ontology ;
                             rdfs:label "Potato diseases, disorders and deficiencies ontology (from UMass, AHDB, INRA, Maurya et al.)"@en .

#################################################################
#    Object Properties
#################################################################

###  http://example.org/potato#hasControlAction
:hasControlAction rdf:type owl:ObjectProperty ;
                  rdfs:range :ControlAction ;
                  rdfs:label "has control action"@en .


###  http://example.org/potato#hasDisease
:hasDisease rdf:type owl:ObjectProperty ;
            rdfs:range :PotatoDisease ;
            rdfs:label "has disease"@en .


###  http://example.org/potato#hasSymptom
:hasSymptom rdf:type owl:ObjectProperty ;
            rdfs:range :Symptom ;
            rdfs:label "has symptom"@en .


###  http://example.org/potato#indicatesDisease
:indicatesDisease rdf:type owl:ObjectProperty ;
                  rdfs:range :PotatoDisease ;
                  rdfs:label "indicates disease"@en .


#################################################################
#    Data properties
#################################################################

###  http://example.org/potato#hasAgentNote
:hasAgentNote rdf:type owl:DatatypeProperty ;
              rdfs:domain owl:Thing ;
              rdfs:range rdfs:Literal ;
              rdfs:label "short note about causal agent"@en .


###  http://example.org/potato#hasDescription
:hasDescription rdf:type owl:DatatypeProperty ;
                rdfs:domain owl:Thing ;
                rdfs:range rdfs:Literal ;
                rdfs:label "description"@en .


###  http://example.org/potato#hasName
:hasName rdf:type owl:DatatypeProperty ;
         rdfs:domain owl:Thing ;
         rdfs:range rdfs:Literal ;
         rdfs:label "human-readable name"@en .


###  http://example.org/potato#severityLevel
:severityLevel rdf:type owl:DatatypeProperty ;
               rdfs:domain owl:Thing ;
               rdfs:range rdfs:Literal ;
               rdfs:label "severity level"@en .


#################################################################
#    Classes
#################################################################

###  http://example.org/potato#AccidentalDisorder
:AccidentalDisorder rdf:type owl:Class ;
                    rdfs:subClassOf :NonParasiticDisorder ;
                    rdfs:label "Accidental / abiotic disorder"@en .


###  http://example.org/potato#BacterialDisease
:BacterialDisease rdf:type owl:Class ;
                  rdfs:subClassOf :ParasiticDisease ;
                  rdfs:label "Bacterial or phytoplasma disease"@en .


###  http://example.org/potato#ControlAction
:ControlAction rdf:type owl:Class ;
               rdfs:label "Control or management action"@en .


###  http://example.org/potato#ExternalPhysiologicalDisorder
:ExternalPhysiologicalDisorder rdf:type owl:Class ;
                               rdfs:subClassOf :PhysiologicalDisorder ;
                               rdfs:label "External physiological disorder"@en .


###  http://example.org/potato#FungalDisease
:FungalDisease rdf:type owl:Class ;
               rdfs:subClassOf :ParasiticDisease ;
               rdfs:label "Fungal / fungal-like disease"@en .


###  http://example.org/potato#InternalPhysiologicalDisorder
:InternalPhysiologicalDisorder rdf:type owl:Class ;
                               rdfs:subClassOf :PhysiologicalDisorder ;
                               rdfs:label "Internal physiological disorder"@en .


###  http://example.org/potato#LeafSymptom
:LeafSymptom rdf:type owl:Class ;
             rdfs:subClassOf :Symptom ;
             rdfs:label "Leaf symptom"@en .


###  http://example.org/potato#MineralDeficiencyDisorder
:MineralDeficiencyDisorder rdf:type owl:Class ;
                           rdfs:subClassOf :PhysiologicalDisorder ;
                           rdfs:label "Mineral deficiency / toxicity related disorder"@en .


###  http://example.org/potato#NematodeDisease
:NematodeDisease rdf:type owl:Class ;
                 rdfs:subClassOf :ParasiticDisease ;
                 rdfs:label "Nematode-related disease"@en .


###  http://example.org/potato#NonParasiticDisorder
:NonParasiticDisorder rdf:type owl:Class ;
                      rdfs:subClassOf :PotatoDisease ;
                      rdfs:label "Non-parasitic disorder"@en .


###  http://example.org/potato#ParasiticDisease
:ParasiticDisease rdf:type owl:Class ;
                  rdfs:subClassOf :PotatoDisease ;
                  rdfs:label "Parasitic disease"@en .


###  http://example.org/potato#PhysiologicalDisorder
:PhysiologicalDisorder rdf:type owl:Class ;
                       rdfs:subClassOf :NonParasiticDisorder ;
                       rdfs:label "Physiological disorder"@en .


###  http://example.org/potato#Plant
:Plant rdf:type owl:Class ;
       rdfs:label "Plant"@en .


###  http://example.org/potato#PotatoDisease
:PotatoDisease rdf:type owl:Class ;
               rdfs:label "Potato disease or disorder"@en .


###  http://example.org/potato#PotatoPlant
:PotatoPlant rdf:type owl:Class ;
             rdfs:subClassOf :Plant ;
             rdfs:label "Potato plant"@en .


###  http://example.org/potato#StemSymptom
:StemSymptom rdf:type owl:Class ;
             rdfs:subClassOf :Symptom ;
             rdfs:label "Stem symptom"@en .


###  http://example.org/potato#Symptom
:Symptom rdf:type owl:Class ;
         rdfs:label "Symptom"@en .


###  http://example.org/potato#TuberSymptom
:TuberSymptom rdf:type owl:Class ;
              rdfs:subClassOf :Symptom ;
              rdfs:label "Tuber symptom"@en .


###  http://example.org/potato#ViroidDisease
:ViroidDisease rdf:type owl:Class ;
               rdfs:subClassOf :ParasiticDisease ;
               rdfs:label "Viroid disease"@en .


###  http://example.org/potato#VirusDisease
:VirusDisease rdf:type owl:Class ;
              rdfs:subClassOf :ParasiticDisease ;
              rdfs:label "Virus disease"@en .


