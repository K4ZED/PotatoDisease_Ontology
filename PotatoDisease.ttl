<http://example.org/potato> a owl:Ontology ;
  rdfs:label "Potato diseases, disorders and deficiencies ontology (from UMass, AHDB, INRA, Maurya et al.)"@en .

:Plant a owl:Class ;
  rdfs:label "Plant"@en .

:PotatoPlant a owl:Class ;
  rdfs:subClassOf :Plant ;
  rdfs:label "Potato plant"@en .

:PotatoDisease a owl:Class ;
  rdfs:label "Potato disease or disorder"@en .

:ParasiticDisease a owl:Class ;
